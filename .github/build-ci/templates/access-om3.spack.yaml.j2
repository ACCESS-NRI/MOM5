# This is an Spack Environment file.
#
# It describes a set of packages to be installed, along with
# configuration settings.
spack:
  specs:
    # ref is filled in by the workflow
    - access-om3 %intel@2021.10.0 target=x86_64
  packages:
    mom5:
      require:
      - '@git.{{ ref }}'
  view: true
  concretizer:
    # Due to different compilers under the same source root
    unify: false
